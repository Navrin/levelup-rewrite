{
  "name": "levelup-rewrite",
  "version": "0.0.1",
  "description": "A re-write of LevelUp, using typescript and modern javascript.",
  "main": "src/index.js",
  "repository": "https://github.com/Navrin/levelup-rewrite.git",
  "author": "Navrin Thomas <navman360@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": " yarn run build && node ./dist/index.js",
    "build": "tsc -p tsconfig.build.json",
    "postinstall": "yarn run build",
    "delete-test-dbs": "del-cli -f testdb/",
    "test": "yarn run delete-test-dbs && mocha -u tdd --full-trace --growl --require ts-node/register ./tests/**/*.ts* && yarn run delete-test-dbs",
    "test:watch": "mocha-typescript-watch"
  },
  "dependencies": {
    "@types/fs-extra": "^3.0.2",
    "chai": "^4.0.0",
    "deferred-leveldown": "^1.2.1",
    "fs-extra": "^3.0.1",
    "level-codec": "^7.0.0",
    "level-errors": "^1.0.4",
    "level-iterator-stream": "^1.3.1",
    "leveldown": "^1.7.0",
    "levelup": "^1.3.7",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "typescript": "^2.3.3"
  },
  "devDependencies": {
    "@alexlur/rollup-plugin-typescript": "^1.0.4",
    "@types/chai": "^3.5.2",
    "@types/leveldown": "^1.7.0",
    "@types/lodash": "^4.14.64",
    "@types/mocha": "^2.2.41",
    "@types/rollup": "^0.41.0",
    "del-cli": "^1.0.0",
    "mocha-typescript": "^1.1.4",
    "node-watch": "^0.5.4",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.4.1",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.0.4",
    "tslint": "^5.3.2",
    "tslint-config-airbnb": "^5.0.1",
    "tslint-eslint-rules": "^4.1.1"
  }
}
